{
  "STRING_VALUE": {
    "pattern": "\"(?:\\\\.|[^\"])*\"",
    "type": "STRING_VALUE"
  },
  "APOSTROPHE_STRING": {
    "pattern": "'(?:\\\\.|[^'])*'",
    "type": "STRING_VALUE"
  },
  "DECLARATION": {
    "pattern": "\\bdeclar\\b",
    "type": "DECLARATION_VARIABLE"
  },
  "OPERATOR_PLUS": {
    "pattern": "\\+",
    "type": "OPERATOR_PLUS"
  },
  "OPERATOR_MINUS": {
    "pattern": "-",
    "type": "OPERATOR_MINUS"
  },
  "\\*": {
    "pattern": "\\*",
    "type": "OPERATOR_MULTIPLY"
  },
  "/": {
    "pattern": "/",
    "type": "OPERATOR_DIVIDE"
  },
  ":": {
    "pattern": ":",
    "type": "DOUBLE_DOTS"
  },
  ";": {
    "pattern": ";",
    "type": "SEPARATOR"
  },
  "=": {
    "pattern": "=",
    "type": "ASSIGNATION"
  },
  "\\(": {
    "pattern": "\\(",
    "type": "LEFT_PARENTHESIS"
  },
  "\\)": {
    "pattern": "\\)",
    "type": "RIGHT_PARENTHESIS"
  },
  "\\b\\w+\\s*\\((?:[^()]*|\\([^()]*\\))*\\)": {
    "pattern": "\\b\\w+\\s*\\((?:[^()]*|\\([^()]*\\))*\\)",
    "type": "METHOD_CALL"
  },
  ",": {
    "pattern": ",",
    "type": "COMA"
  },
  "\\bnumber\\b": {
    "pattern": "\\bnumber\\b",
    "type": "NUMBER_TYPE"
  },
  "\\bstring\\b": {
    "pattern": "\\bstring\\b",
    "type": "STRING_TYPE"
  },
  "\\b\\d+\\.?\\d*\\b": {
    "pattern": "\\b\\d+\\.?\\d*\\b",
    "type": "NUMBER_VALUE"
  },
  "(?<!\")\\b[a-zA-Z_][a-zA-Z0-9_]*\\b(?!\")": {
    "pattern": "(?<!\")\\b[a-zA-Z_][a-zA-Z0-9_]*\\b(?!\")",
    "type": "VARIABLE_NAME"
  }
}